{
  "name": "@navvi/slashauth-react-client",
  "version": "0.1.1",
  "description": "React client for SlashAuth. Add auth to your web3 dApp",
  "main": "index.js",
  "scripts": {
    "dev": "rimraf dist && rollup -c --watch",
    "start": "npm run dev",
    "build": "rimraf dist && rollup -m -c --environment NODE_ENV:production && npm run test:es-check",
    "build:stats": "npm run build -- --environment WITH_STATS:true && open stats.html",
    "_postinstall": "husky install",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "lint": "npx eslint --ext .ts --ext .tsx --max-warnings 0 .",
    "lint-fix": "npx eslint --ext .ts --ext .tsx --max-warnings 0 --fix ./src"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-replace": "^4.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/react": "18.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "assert": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "chai": "^4.3.6",
    "eslint": "8.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.6",
    "oidc-provider": "^7.10.6",
    "prettier": "^2.6.0",
    "react-is": "^18.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dev": "^2.0.3",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "rollup-plugin-web-worker-loader": "^1.6.1",
    "typescript": "4.5.5"
  },
  "peer-dependencies": {
    "react": ">=17.x"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "npx eslint --ext .ts --ext .tsx --fix --max-warnings 0 --"
    ]
  },
  "dependencies": {
    "@navvi/react-use-metamask": "^0.1.8",
    "browser-tabs-lock": "^1.2.15",
    "es-cookie": "^1.3.2",
    "react": "^18.0.0",
    "uuid": "^8.3.2"
  }
}
